{
  "name": "Alert",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date",
      "defaultFn": "now",
      "required": true
    },
    "resolved": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "location": {
      "type": "geopoint",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "appUser": {
      "type": "belongsTo",
      "model": "AppUser",
      "foreignKey": ""
    },
    "responders": {
      "type": "hasAndBelongsToMany",
      "model": "AppUser",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
