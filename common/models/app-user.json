{
  "name": "AppUser",
  "plural": "appusers",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "excludeBaseProperties": [
    "email",
    "emailVerified",
    "realm"
  ],
  "properties": {
    "phone": {
      "type": "string",
      "required": true
    },
    "firstname": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "location": {
      "type": "geopoint"
    }
  },
  "validations": [],
  "relations": {
    "alerts": {
      "type": "hasMany",
      "model": "Alert",
      "foreignKey": ""
    },
    "responses": {
      "type": "hasMany",
      "model": "Alert",
      "foreignKey": "appUserId",
      "through": "Responder"
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__alerts"
    }
  ],
  "methods": {}
}
